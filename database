/* 

DATABASES

*/
  
-- display the available databases.
SHOW DATABASES;

-- create a database modelone.
CREATE DATABASE modelone;

-- create a database modelone if it doesn't exist.
CREATE DATABASE IF NOT EXISTS modelone;

-- select the database. A database must be selected before it can be used.
USE modelone;

-- delete the database. The database will no longer show up in the list of databases.
DROP DATABASE modelone;
